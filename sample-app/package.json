{
  "name": "@jackvcurtis/express-react-ssr-sample-app",
  "version": "0.1.0",
  "description": "\"A sample app for the express-react-ssr-engine\"",
  "main": "build/index.js",
  "scripts": {
    "test": "jest -i",
    "build": "tsc && npm run copy-all",
    "copy-all": "copyfiles server/views/templates build/views && copyfiles -r server/views/styles build/views && copyfiles -r server/components/styles build/components",
    "start": "ts-node server",
    "clean": "rm -r server/dist && rm -r build"
  },
  "keywords": [
    "express",
    "express-react-ssr-engine"
  ],
  "author": "Jack Curtis",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/types": "^7.15.0",
    "@jackvcurtis/express-react-ssr-engine": "^0.3.4",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.0.2",
    "copyfiles": "^2.4.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.4",
    "react-dom": "^17.0.2",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "throng": "^5.0.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@testing-library/react": "^12.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.0",
    "@types/node": "^16.6.1",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__react": "^10.2.0",
    "jest": "^27.0.6",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0"
  }
}
